Private Sub Form_Open(Cancel As Integer)
If strExcel_Folder = "" Then
    strExcel_Folder = Environ("USERPROFILE") & "\Desktop"
End If
If strHTML_Folder = "" Then
    strHTML_Folder = Environ("USERPROFILE") & "\Desktop"
End If

If strTemplate_Folder = "" Then
    strTemplate_Folder = Environ("USERPROFILE") & "\Desktop"
End If

End Sub

Private Sub TemplateFile_MouseDown(Button As Integer, Shift As Integer, X As Single, Y As Single)
   If Button = 1 Then
            Dim strFile As String
            strFile = GetFileName(strTemplate_Folder)
            If Not strFile = "" Then
                'strip path
                strFile = FileNameFromPath(strFile)
                Me.TemplateFile = strFile
            End If
    End If
End Sub
